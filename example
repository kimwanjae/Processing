int flag;
int count;
void setup(){
  size(400,400);
  background(255);
}
void draw(){
   if(mousePressed){  
    if(mouseX>=150 && mouseX<=250&&mouseY>=150 && mouseY<=250){
       count++;
     }
       fill(150,0,0);
    } 
    else{
       fill(255,0,0);
    }
  rect(150,150,100,100);
  fill(255,255,0);
  textSize(20);
  text("button",165,200);
  
  if(count % 2==0) {
    fill(255,255,0);
  }
  else{
    fill(0);
  }
  ellipse(200,100,50,50);
  }
